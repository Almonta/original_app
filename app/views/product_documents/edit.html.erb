<div class="container">
  <div class="product_document_page">
    <div class="set_center_outer">
      <h2 class="page_title_en">Edit Product Document</h2>
      <h5 class="page_title_ja"><%= t("views.title.editing_product_documents") %></h5>
    </div>

      <%= render "form", { product: @product, product_document: @product_document } %>

    <div class="set_center_outer">
      <%= link_to t("views.button.back"), :back, class: "hover_background_none" %>|
      <%= link_to t("views.button.show_product_document"), product_product_document_path(@product, @product_document), class: "hover_background_none" %>
    </div>
  </div>
</div>